{  "build": {
    "env": {
      "NODE_ENV": "production"
    },
    "script": "npm run storybook:build"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/storybook-static/$1"
    }
  ],
  "outputDirectory": "storybook-static"
    "routes": [{ "handle": "filesystem" }, { "src": "/(.*)", "dest": "/index.html" }]
}
